---
import { Icon } from 'astro-icon/components';
import { SITE } from '~/utils/config';

interface Link {
  text?: string;
  href?: string;
  ariaLabel?: string;
  icon?: string;
}

interface Links {
  title?: string;
  links: Array<Link>;
}

interface Addresses {
  title: string;
  address: string;
  map: { id: string; title: string; latlng: string };
  tel: { text: string; href: string }[];
}

export interface Props {
  addresses: Array<Addresses>;
  links?: Array<Links>;
  secondaryLinks?: Array<Link>;
  socialLinks: Array<Link>;
  footNote?: string;
  theme?: string;
}

const { addresses = [], socialLinks = [], secondaryLinks = [], footNote = '', theme = 'light' } = Astro.props;
---

<footer class:list={[{ dark: theme === 'dark' }, 'not-prose relative border-t border-gray-200 dark:border-slate-800']}>
  <div class="dark:bg-dark pointer-events-none absolute inset-0" aria-hidden="true"></div>
  <div class="relative mx-auto max-w-7xl px-4 dark:text-slate-300 sm:px-6">
    <div class="grid grid-cols-6 gap-4 gap-y-8 py-8 sm:gap-8 md:py-12">
      <div class="col-span-6 md:col-span-2 md:col-start-2 lg:col-start-1">
        <div class="mb-2">
          <p class="inline-block text-xl font-bold">{SITE?.name}</p>
        </div>
        <div class="flex flex-col text-sm text-muted">
          {
            secondaryLinks.map(({ text, href }) => (
              <a
                class="my-1 mr-2 text-base text-muted transition duration-150 ease-in-out hover:text-gray-700 hover:underline rtl:ml-2 rtl:mr-0 dark:text-gray-400"
