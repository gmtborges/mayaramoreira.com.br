              <div class="mb-2 text-lg font-medium dark:text-gray-300">{title}</div>
              <p class="text-muted dark:text-gray-400" set:html={address} />
              <>
                {tel.map((t) => (
                  <a
                    class="my-1 mr-2 block text-base text-muted transition duration-150 ease-in-out hover:text-gray-700 hover:underline rtl:ml-2 rtl:mr-0 dark:text-gray-400"
                    href={t.href}
                    set:html={t.text}
                  />
                ))}
              </>
            </div>
          ))
        }
